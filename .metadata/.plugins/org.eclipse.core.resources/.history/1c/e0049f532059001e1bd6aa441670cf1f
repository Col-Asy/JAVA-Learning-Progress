package Assg2;

public class series_print {
	public static void series_print(int n, int m) {
		int i=1;
		while(i<=n) {
			if(3*i+2%4!=0) {
				System.out.println(3*i+2);
				i++;
			}
		}
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n=10;
		int m=4;
		series_print(n,m);
	}

}
